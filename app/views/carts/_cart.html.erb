<div class="col l4 z-depth-4">
  <p>/app/views/carts/_cart.html.erb</p>
  <table class="hoverable">
    <%= render(cart.line_items) %>    
    <tr class="total_line">
      <td colspan="3">Total</td>
      <td class="total_cell right-align"><%= cart.total_price %></td>
    </tr>
  </table>
    <%= button_to 'Empty cart', cart, method: :delete,
      data: { confirm: 'Are you sure?' } %>
</div>